{
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 5
  },
  "globals": {
    "bento": true,
    "window": true, 
    "document": true, 
    "console": true, 
    "localStorage": true, 
    "navigator": true, 
    "Image": true
  },
  "rules": {
    "no-shadow": [
      2,
      {
        "builtinGlobals": true,
        "hoist": "all"
      }
    ],
    "no-console": 0,
    "no-irregular-whitespace": 0,
    "no-empty": 0,
    "no-loop-func": 0,
    "no-undef": 2,
    "dot-notation": 0,
    "no-unused-vars": 0,
    "semi": 1,
    "one-var": ["error", "never"]
  }
}